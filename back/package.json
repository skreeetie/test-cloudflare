{
	"name": "cf-web-monorepo",
	"version": "1.0.0",
	"description": "Cloudflare Web Application Monorepo",
	"license": "MIT",
	"author": "whiletruedoend@ya.ru",
	"type": "module",
	"keywords": [
		"react",
		"boilerplate",
		"typescript",
		"starter",
		"vite",
		"redux",
		"material-ui",
		"rrd",
		"prettier"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/StalkerRaftik/cf-web-monorepo.git"
	},
	"bugs": {
		"url": "https://github.com/StalkerRaftik/cf-web-monorepo/issues"
	},
	"homepage": "https://github.com/StalkerRaftik/cf-web-monorepo#readme",
	"scripts": {
		"deploy": "tsc && vite build && wrangler deploy",
		"deploy:dev": "tsc && vite build && wrangler deploy --env dev",
		"deploy:test": "tsc && vite build && wrangler deploy --env test",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"makemigrations": "drizzle-kit generate",
		"migrate": "wrangler d1 migrations apply %npm_config_db% --local",
		"migrate:dev": "wrangler d1 migrations apply %npm_config_db% --env dev --remote",
		"migrate:test": "wrangler d1 migrations apply %npm_config_db% --env test --remote",
		"migrate:prod": "wrangler d1 migrations apply %npm_config_db% --remote",
		"cf-typegen": "wrangler types",
		"fdev": "vite --open",
		"fstart": "vite --open",
		"build": "tsc && vite build",
		"lint": "eslint src --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
		"format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cover": "vitest run --coverage",
		"test:e2e": "pnpm preview:test 'cypress open'",
		"test:e2e:headless": "pnpm preview:test 'cypress run'",
		"test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'"
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/icons-material": "^6.1.3",
		"@mui/material": "^6.1.3",
		"@reduxjs/toolkit": "^2.2.8",
		"chanfana": "^2.5.2",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.39.3",
		"i18next": "^24.2.0",
		"i18next-browser-languagedetector": "^8.0.2",
		"i18next-http-backend": "^3.0.1",
		"itty-router": "^5.0.18",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-i18next": "^15.2.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.27.0",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@types/node": "^20.8.3",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@types/service-worker-mock": "^2.0.1",
		"@vitejs/plugin-react-swc": "^3.7.1",
		"drizzle-kit": "^0.30.4",
		"eslint": "^9.18.0",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.12",
		"globals": "^15.11.0",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.8.1",
		"vite": "^5.4.8",
		"wrangler": "^4.43.0"
	},
	"optionalDependencies": {
		"@swc/core-darwin-arm64": "1.10.3",
		"@swc/core-darwin-x64": "1.10.3",
		"@swc/core-linux-arm-gnueabihf": "1.10.3",
		"@swc/core-linux-arm64-gnu": "1.10.3",
		"@swc/core-linux-arm64-musl": "1.10.3",
		"@swc/core-linux-x64-gnu": "1.10.3",
		"@swc/core-linux-x64-musl": "1.10.3",
		"@swc/core-win32-arm64-msvc": "1.10.3",
		"@swc/core-win32-ia32-msvc": "1.10.3",
		"@swc/core-win32-x64-msvc": "1.10.3"
	}
}